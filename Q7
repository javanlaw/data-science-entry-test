class Car:
    """
    - Class with attributes: make, model, year
    - Initializes attributes in __init__
    - Method describe_car() prints "Year Make Model"
    """
    def __init__(self, make, model, year):
        self.make = make
        self.model = model
        self.year = year

    def describe_car(self):
        print(f"{self.year} {self.make} {self.model}")

# Create instance and Call describe_car
car1 = Car("Toyota", "Corolla", 2020)
car1.describe_car()  

# Output: 2020 Toyota Corolla
